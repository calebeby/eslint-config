{
  "name": "eslint-plugin-caleb",
  "version": "10.0.2",
  "description": "Caleb Eby's shareable ESLint configuration",
  "author": "Caleb Eby <caleb.eby01@gmail.com>",
  "homepage": "https://github.com/calebeby/eslint-config",
  "bugs": "https://github.com/calebeby/eslint-config/issues",
  "license": "MIT",
  "main": "index.js",
  "repository": "calebeby/eslint-config",
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "files": [
    "dist"
  ],
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "3.9.0",
    "@typescript-eslint/parser": "3.9.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jest": "23.20.0",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.20.5",
    "eslint-plugin-react-hooks": "4.0.8",
    "eslint-plugin-standard": "4.0.1",
    "eslint-plugin-unicorn": "21.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.11.1",
    "@babel/plugin-transform-shorthand-properties": "7.10.4",
    "@changesets/changelog-github": "0.2.6",
    "@changesets/cli": "2.10.0",
    "@rollup/plugin-babel": "5.1.0",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "8.4.0",
    "@shopify/eslint-plugin": "37.0.1",
    "babel-plugin-un-cjs": "2.0.0",
    "eslint": "7.6.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-config-standard": "14.1.1",
    "eslint-config-xo": "0.32.1",
    "prettier": "2.0.5",
    "rollup": "2.23.1",
    "rollup-plugin-prettier": "2.1.0",
    "rollup-plugin-terser": "7.0.0",
    "typescript": "3.9.7"
  },
  "peerDependencies": {
    "eslint": "^7.0.0",
    "prettier": ">=1"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "all",
    "proseWrap": "always"
  },
  "scripts": {
    "check-lint": "eslint . && prettier --check .",
    "lint": "eslint --fix . && prettier --write .",
    "build": "rollup -c rollup.config.js && node build.js",
    "build:config": "node build.js",
    "changeset": "changeset",
    "version": "changeset version && prettier --write .",
    "release": "npm run build && changeset publish"
  },
  "eslintConfig": {
    "extends": [
      "plugin:caleb/recommended",
      "plugin:caleb/preact"
    ]
  },
  "eslintIgnore": [
    "dist"
  ],
  "engines": {
    "node": ">=8.3.0"
  }
}

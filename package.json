{
  "name": "eslint-plugin-caleb",
  "version": "0.0.0-semantically-released",
  "description": "Caleb Eby's shareable ESLint configuration",
  "author": "Caleb Eby <caleb.eby01@gmail.com>",
  "homepage": "https://github.com/calebeby/eslint-config",
  "bugs": "https://github.com/calebeby/eslint-config/issues",
  "license": "MIT",
  "main": "index.js",
  "repository": "calebeby/eslint-config",
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "files": [
    "dist"
  ],
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "2.8.0",
    "@typescript-eslint/parser": "2.8.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-jest": "22.21.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-node": "10.0.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.17.0",
    "eslint-plugin-react-hooks": "2.5.0",
    "eslint-plugin-standard": "4.0.1",
    "eslint-plugin-unicorn": "12.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.8.7",
    "@babel/plugin-proposal-object-rest-spread": "7.8.3",
    "@babel/plugin-transform-shorthand-properties": "7.8.3",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-config-standard": "14.1.0",
    "eslint-config-xo": "0.27.2",
    "eslint-plugin-shopify": "31.0.0",
    "prettier": "1.19.1",
    "rollup": "1.32.1",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-json": "4.0.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-prettier": "0.6.0",
    "rollup-plugin-terser": "5.2.0",
    "typescript": "3.8.3",
    "semantic-release": "15.14.0"
  },
  "peerDependencies": {
    "eslint": "^6",
    "prettier": ">=1"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "all",
    "proseWrap": "always"
  },
  "release": {
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "angular",
          "parserOpts": {
            "noteKeywords": [
              "BREAKING CHANGE",
              "BREAKING CHANGES",
              "BREAKING"
            ]
          }
        }
      ],
      "@semantic-release/github",
      "@semantic-release/npm",
      "@semantic-release/release-notes-generator"
    ]
  },
  "scripts": {
    "check-lint": "eslint --ignore-path .gitignore . && prettier --list-different --ignore-path .gitignore '**/*.js'",
    "lint": "eslint --ignore-path .gitignore --fix . && prettier --write --ignore-path .gitignore '**/*.js'",
    "prepublishOnly": "npm run build && npm run check-lint",
    "build": "rollup -c rollup.config.js && node build.js",
    "build:config": "node build.js",
    "semantic-release": "semantic-release"
  },
  "eslintConfig": {
    "extends": [
      "plugin:caleb/recommended",
      "plugin:caleb/preact"
    ]
  },
  "eslintIgnore": [
    "dist"
  ],
  "engines": {
    "node": ">=8.3.0"
  }
}

{
  "name": "eslint-plugin-caleb",
  "version": "0.0.0-semantically-released",
  "description": "Caleb Eby's shareable ESLint configuration",
  "author": "Caleb Eby <caleb.eby01@gmail.com>",
  "homepage": "https://github.com/calebeby/eslint-config",
  "bugs": "https://github.com/calebeby/eslint-config/issues",
  "license": "MIT",
  "main": "index.js",
  "repository": "calebeby/eslint-config",
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "files": [
    "dist"
  ],
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "2.34.0",
    "@typescript-eslint/parser": "2.34.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jest": "23.13.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.20.0",
    "eslint-plugin-react-hooks": "4.0.0",
    "eslint-plugin-standard": "4.0.1",
    "eslint-plugin-unicorn": "20.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.10.2",
    "@babel/plugin-transform-shorthand-properties": "7.10.1",
    "@rollup/plugin-babel": "5.0.3",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "7.1.3",
    "@shopify/eslint-plugin": "36.1.0",
    "babel-plugin-un-cjs": "2.0.0",
    "eslint": "7.1.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-config-standard": "14.1.1",
    "eslint-config-xo": "0.30.0",
    "prettier": "2.0.5",
    "rollup": "2.13.1",
    "rollup-plugin-prettier": "2.1.0",
    "rollup-plugin-terser": "5.3.0",
    "semantic-release": "17.0.8",
    "typescript": "3.9.5"
  },
  "peerDependencies": {
    "eslint": "^6 || ^7.0.0",
    "prettier": ">=1"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "all",
    "proseWrap": "always"
  },
  "release": {
    "branches": [
      "master"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "angular",
          "parserOpts": {
            "noteKeywords": [
              "BREAKING CHANGE",
              "BREAKING CHANGES",
              "BREAKING"
            ]
          }
        }
      ],
      "@semantic-release/github",
      "@semantic-release/npm",
      "@semantic-release/release-notes-generator"
    ]
  },
  "scripts": {
    "check-lint": "eslint . && prettier --check .",
    "lint": "eslint --fix . && prettier --write .",
    "build": "rollup -c rollup.config.js && node build.js",
    "build:config": "node build.js",
    "semantic-release": "semantic-release"
  },
  "eslintConfig": {
    "extends": [
      "plugin:caleb/recommended",
      "plugin:caleb/preact"
    ]
  },
  "eslintIgnore": [
    "dist"
  ],
  "engines": {
    "node": ">=8.3.0"
  }
}
